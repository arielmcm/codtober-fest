{
  "name": "f7-projects-api",
  "version": "1.0.0",
  "main": "server/server.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "lint": "eslint ./app",
    "start": "node .",
    "posttest": "npm run lint",
    "precommit": "eslint ./app  && jest",
    "model": "node model-generator.js",
    "test": "node scripts/test.js --env=node",
    "test-watch": "node_modules/nodemon/bin/nodemon.js node_modules/jasmine/bin/jasmine.js"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "helmet": "^3.13.0",
    "i": "^0.3.5",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "loopback": "^3.4.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^4.0.0",
    "loopback-connector-mongodb": "^1.18.1",
    "netmask": "^1.0.6",
    "npm": "^5.0.3",
    "serve-favicon": "^2.0.1",
    "strong-error-handler": "^2.0.0",
    "useragent": "^2.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "dotenv": "^4.0.0",
    "eslint": "^4.0.0",
    "eslint-config-loopback": "^8.0.0",
    "husky": "^0.13.3",
    "jest": "20.0.4",
    "jest-teamcity-reporter": "^0.6.2",
    "nodemon": "^1.11.0",
    "nsp": "^2.1.0",
    "to-pascal-case": "^1.0.0"
  },
  "jest": {
    "coverageReporters": [
      "teamcity",
      "text"
    ],
    "testResultsProcessor": "jest-teamcity-reporter",
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "license": "UNLICENSED",
  "description": "contest-backend"
}
